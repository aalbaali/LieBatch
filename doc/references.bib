  file      = {IEEE Xplore Abstract Record:C\:\\Users\\albaa\\Zotero\\storage\\QZSD9B49\\5652603.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\albaa\\Zotero\\storage\\PJBRXFJW\\Huang et al. - 2010 - How far is SLAM from a linear least squares proble.pdf:application/pdf},
  keywords  = {Simultaneous localization and mapping, SLAM (robots), nonlinear estimation, nonlinear programming, Vectors, Covariance matrix, Optimization, Robot kinematics, least squares approximations, complex nonlinear estimation problem, complex nonlinear optimization problem, iterative methods, linear least squares problem, nonlinear least squares problem, Rail to rail inputs, SLAM problem},
