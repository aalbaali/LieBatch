  journal  = {IEEE Transactions on Intelligent Transportation Systems},
  title    = {High-{Precision} {Vehicle} {Navigation} in {Urban} {Environments} {Using} an {MEM}'s {IMU} and {Single}-{Frequency} {GPS} {Receiver}},
