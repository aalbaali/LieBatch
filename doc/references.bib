@Article{Jiang_Bregman_,
  author   = {Jiang, Xin and Vandenberghe, Lieven},
  title    = {Bregman primal–dual ﬁrst-order method and application to sparse semideﬁnite programming},
  pages    = {29},
  abstract = {We present a new variant of the Chambolle–Pock primal–dual method with Bregman distances, analyze its convergence, and apply it to the centering problem in sparse semideﬁnite programming. The novelty in the method is a line search procedure for selecting suitable step sizes. The line search obviates the need for estimating the norm of the constraint matrix and the strong convexity constant of the Bregman kernel. As an application, we discuss the centering problem in large-scale semideﬁnite programming with sparse coeﬃcient matrices. The logarithmic barrier function for the cone of positive semideﬁnite completable sparse matrices is used as the distance-generating kernel. For this distance, the complexity of evaluating the Bregman proximal operator is shown to be roughly proportional to the cost of a sparse Cholesky factorization. This is much cheaper than the standard proximal operator with Euclidean distances, which requires an eigenvalue decomposition.},
  file     = {Jiang and Vandenberghe - Bregman primal–dual ﬁrst-order method and applicat.pdf:C\:\\Users\\albaa\\Zotero\\storage\\CHG7Y8CL\\Jiang and Vandenberghe - Bregman primal–dual ﬁrst-order method and applicat.pdf:application/pdf},
  language = {en},
}

