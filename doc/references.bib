@Article{GomezOjeda_PL_2018,
  author     = {Gomez-Ojeda, Ruben and Zuñiga-Noël, David and Moreno, Francisco-Angel and Scaramuzza, Davide and Gonzalez-Jimenez, Javier},
  journal    = {arXiv:1705.09479 [cs]},
  title      = {{PL}-{SLAM}: a {Stereo} {SLAM} {System} through the {Combination} of {Points} and {Line} {Segments}},
  year       = {2018},
  month      = apr,
  note       = {arXiv: 1705.09479},
  abstract   = {Traditional approaches to stereo visual SLAM rely on point features to estimate the camera trajectory and build a map of the environment. In lowtextured environments, though, it is often diﬃcult to ﬁnd a suﬃcient number of reliable point features and, as a consequence, the performance of such algorithms degrades. This paper proposes PL-SLAM, a stereo visual SLAM system that combines both points and line segments to work robustly in a wider variety of scenarios, particularly in those where point features are scarce or not well-distributed in the image. PLSLAM leverages both points and segments at all the instances of the process: visual odometry, keyframe selection, bundle adjustment, etc. We contribute also with a loop closure procedure through a novel bag-ofwords approach that exploits the combined descriptive power of the two kinds of features. Additionally, the resulting map is richer and more diverse in 3D elements, which can be exploited to infer valuable, highlevel scene structures like planes, empty spaces, ground plane, etc. (not addressed in this work). Our proposal has been tested with several popular datasets (such as KITTI and EuRoC), and is compared to state of the art methods like ORB-SLAM, revealing a more robust performance in most of the experiments, while still running in real-time. An open source version of the PL-SLAM C++ code will be released for the beneﬁt of the community.},
  file       = {Gomez-Ojeda et al. - 2018 - PL-SLAM a Stereo SLAM System through the Combinat.pdf:C\:\\Users\\albaa\\Zotero\\storage\\4SSBITIR\\Gomez-Ojeda et al. - 2018 - PL-SLAM a Stereo SLAM System through the Combinat.pdf:application/pdf},
  keywords   = {Computer Science - Computer Vision and Pattern Recognition},
  language   = {en},
  shorttitle = {{PL}-{SLAM}},
  url        = {http://arxiv.org/abs/1705.09479},
  urldate    = {2021-03-15},
}

@Article{Walsh_Underwater_2019a,
  author   = {Walsh, Alex and Arsenault, Jonathan and Forbes, James Richard},
  title    = {Underwater {Navigation} using the {Invariant} {Extended} {Kalman} {Filter} {Framework}},
  year     = {2019},
  number   = {19},
  pages    = {15},
  abstract = {In underwater navigation applications, the extended Kalman ﬁlter (EKF) has historically been the ﬁlter of choice. However, the EKF error dynamics are generally state dependent, resulting in poor convergence properties and, in some cases, divergence of the state estimate. The invariant EKF (IEKF) deﬁnes the error dynamics in a state independent way resulting in much more robust and reliable state estimates. This paper’s contribution is the application of the IEKF framework to the underwater navigation problem. Due to the nature of kinematics and sensors in this problem, the IEKF framework cannot be directly applied. It is shown that it is best to prioritize the preservation of the invariant properties of the measurement model over the preservation of the group-afﬁne properties of the process model when implementing an IEKF. Three approximate IEKFs are presented and are compared to traditional EKFs in both simulation and experiments. The results presented in this paper show that the IEKF framework realizes improved state estimates compared to traditional EKFs, even in the case of an approximative IEKF implementation where the error dynamics are not group afﬁne.},
  file     = {Walsh et al. - 2019 - Underwater Navigation using the Invariant Extended.pdf:C\:\\Users\\albaa\\Zotero\\storage\\9B7UNKJH\\Walsh et al. - 2019 - Underwater Navigation using the Invariant Extended.pdf:application/pdf},
