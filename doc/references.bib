@Article{Kenway_Scalable_2014,
  author   = {Kenway, Gaetan K. W. and Kennedy, Graeme J. and Martins, Joaquim R. R. A.},
  journal  = {AIAA Journal},
  title    = {Scalable {Parallel} {Approach} for {High}-{Fidelity} {Steady}-{State} {Aeroelastic} {Analysis} and {Adjoint} {Derivative} {Computations}},
  year     = {2014},
  issn     = {0001-1452, 1533-385X},
  month    = may,
  number   = {5},
  pages    = {935--951},
  volume   = {52},
  abstract = {Aeroelastic systems achieve the best performance when the aerodynamic shape and structural sizing are optimized concurrently, but such an optimization is challenging when high-ﬁdelity aerodynamic and structural models are required. This paper addresses this challenge through several signiﬁcant improvements. Fully coupled Newton–Krylov methods are presented for the solution of aerostructural systems and for the corresponding adjoint systems. The coupled adjoint method presented can compute gradients with respect to thousands of multidisciplinary design variables accurately and eﬃciently. This is enabled by several improvements in the computation of the multidisciplinary terms in the coupled adjoint. The parallel scalability of the methods is demonstrated for a full aircraft conﬁguration using an Euler computational ﬂuid dynamics model with more than 8 × 106 state variables and a detailed structural ﬁnite element model of the wing with more than 1×106 degrees of freedom. The coupled Newton–Krylov methods are shown to improve the convergence rate of both the aerostructural solution and the coupled adjoint derivative computations. Gradient computations of aerodynamic and structural functions with respect to both aerodynamic shape and structural sizing variables are veriﬁed, and scaling is demonstrated to O(103) variables. The accuracy and scalability of the presented methods make it possible to perform aerostructural optimizations of full aircraft conﬁgurations with respect to hundreds of external shape and structural sizing design variables, leading to optimal aeroelastic tailoring.},
  doi      = {10.2514/1.J052255},
  file     = {Kenway et al. - 2014 - Scalable Parallel Approach for High-Fidelity Stead.pdf:C\:\\Users\\albaa\\Zotero\\storage\\RWBFRNQ7\\Kenway et al. - 2014 - Scalable Parallel Approach for High-Fidelity Stead.pdf:application/pdf},
  language = {en},
  url      = {http://arc.aiaa.org/doi/10.2514/1.J052255},
  urldate  = {2020-03-16},
}

@Article{Cramer_Problem_1994,
  author   = {Cramer, Evin J. and Dennis, J. E., Jr. and Frank, Paul D. and Lewis, Robert Michael and Shubin, Gregory R.},
  journal  = {SIAM Journal on Optimization},
  title    = {Problem {Formulation} for {Multidisciplinary} {Optimization}},
  year     = {1994},
