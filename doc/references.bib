  file       = {IEEE Xplore Abstract Record:C\:\\Users\\albaa\\Zotero\\storage\\9ICDM3FY\\7782863.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\albaa\\Zotero\\storage\\MBXJVS6C\\Forster et al. - 2017 - SVO Semidirect Visual Odometry for Monocular and .pdf:application/pdf},
  keywords   = {Robustness, sensor fusion, simultaneous localization and mapping (SLAM), Feature extraction, Optimization, Cameras, image texture, Real-time systems, Tracking, feature-based methods, high-frequency texture, image intensity gradients, monocular systems, multicamera systems, probabilistic depth estimation algorithm, Robot vision, semidirect visual odometry, SVO, Visualization},
  shorttitle = {{SVO}},
}

@InProceedings{Srinivasan_Structural_2015,
  author    = {Srinivasan, Natesh and Carlone, Luca and Dellaert, Frank},
  booktitle = {Procedings of the {British} {Machine} {Vision} {Conference} 2015},
  title     = {Structural {Symmetries} from {Motion} for {Scene} {Reconstruction} and {Understanding}},
