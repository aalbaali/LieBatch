  file      = {IEEE Xplore Abstract Record:C\:\\Users\\albaa\\Zotero\\storage\\WAV9AJEE\\6942932.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\albaa\\Zotero\\storage\\Y8WWPSVB\\Khosoussi et al. - 2014 - Novel insights into the impact of graph structure .pdf:application/pdf},
