@Article{Sibley_Sliding_2010,
  author    = {Sibley, Gabe and Matthies, Larry and Sukhatme, Gaurav},
  journal   = {Journal of Field Robotics},
  title     = {Sliding window filter with application to planetary landing},
  year      = {2010},
  issn      = {1556-4967},
  note      = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/rob.20360},
  number    = {5},
  pages     = {587--608},
  volume    = {27},
  abstract  = {We are concerned with improving the range resolution of stereo vision for entry, descent, and landing (EDL) missions to Mars and other planetary bodies. The goal is to create accurate and precise three-dimensional planetary surface-structure estimates by filtering sequences of stereo images taken from an autonomous landing vehicle. We describe a sliding window filter (SWF) approach based on delayed state marginalization. The SWF can run in constant time, yet still achieve experimental results close to those of the bundle adjustment solution. This technique can scale from the offline batch least-squares solution to fast online incremental solutions. For instance, if the window encompasses all time, the solution is equivalent to full bundle adjustment; if only one time step is maintained, the solution matches the extended Kalman filter; if poses and landmarks are slowly marginalized out over time such that the state vector ceases to grow, then the filter becomes constant time, like visual odometry. Within the constant time regime, the sliding window approach demonstrates convergence properties that are close to those of the full batch solution and strictly superior to visual odometry. Experiments with real data show that ground structure estimates follow the expected convergence pattern that is predicted by theory. These experiments indicate the effectiveness of filtering long-range stereo for EDL. © 2010 Wiley Periodicals, Inc.},
  copyright = {Copyright © 2010 Wiley Periodicals, Inc.},
  doi       = {10.1002/rob.20360},
  file      = {Full Text PDF:C\:\\Users\\albaa\\Zotero\\storage\\3QEYEBRS\\Sibley et al. - 2010 - Sliding window filter with application to planetar.pdf:application/pdf;Snapshot:C\:\\Users\\albaa\\Zotero\\storage\\TFHMXSE8\\rob.html:text/html},
  language  = {en},
  url       = {http://onlinelibrary.wiley.com/doi/abs/10.1002/rob.20360},
  urldate   = {2020-07-07},
}

