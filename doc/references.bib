@Article{Zhao_Analysis_2016,
  author   = {Zhao, Lin and Qiu, Haiyang and Feng, Yanming},
  journal  = {Measurement},
  title    = {Analysis of a robust {Kalman} filter in loosely coupled {GPS}/{INS} navigation system},
  year     = {2016},
